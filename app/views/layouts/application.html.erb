<!DOCTYPE html>
<html>
<head>
  <title>cell≈ç-dev</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag "favicon.ico" %>
</head>
<body>

<nav class="navbar navbar-default" role="navigation">
  <div class="container-fluid">
    <div class="navbar-header">
      <%= link_to image_tag("cellodev-logo.png", alt: "cello-dev logo"), root_url, class: "navbar-brand" %>
    </div>

    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
      </ul>
      <ul class="nav navbar-nav navbar-right">
        <% if current_user %>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <span class="glyphicon glyphicon-th-list"></span>
              <b class="caret"></b></a>
            <ul class="dropdown-menu">
              <li><%= link_to "All Projects", root_url %></li>
              <li class="divider"></li>
              <li><%= link_to "Sign Out", session_url, method: :delete %></li>
            </ul>
          </li>
        <% else %>
        <% end %>
        <li class="uppercase"><%= link_to "about", "https://github.com/tpeg88/cello-dev" %></li>
      </ul>
    </div>
  </div>
</nav>

<% if flash[:errors] %>
<div class="home-errors pull-right">
  <% flash[:errors].each do |error| %>
  <p>- <%= error %></p>
  <% end %>
</div>
<% end %>


  <%= yield %>


</body>
</html>
